<?php

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function custom_api_data_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.custom_api_data':
      return '<p>' . t('Custom API Data module for fetching and displaying data from external APIs.') . '</p>';
  }
}
